{
  "private": false,
  "license": "MIT",
  "name": "react-babylonjs-3d",
  "version": "0.0.1-alpha4",
  "description": "BabylonJS 3D in a declarative way: CycleJS, ReactJS, RxJS",
  "main": "index.js",
  "scripts": {
    "test-only": "mocha --opts test/mocha.opts",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "nyc mocha --opts test/mocha.opts",
    "prettier": "prettier --config ./.prettierrc --write \"{src,test,mocks,examples}/**/*.js\""
  },
  "nyc": {
    "watermarks": {
      "lines": [
        80,
        95
      ],
      "functions": [
        80,
        95
      ],
      "branches": [
        80,
        95
      ],
      "statements": [
        80,
        95
      ]
    },
    "exclude": [
      "**/*.spec.js",
      "examples",
      "test"
    ],
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viniciusrmcarneiro/react-babylonjs.git"
  },
  "engines": {
    "node": ">= 8.2.1",
    "npm": ">= 5.30.0"
  },
  "author": "Vinicius R. M. Carneiro",
  "dependencies": {
    "ramda": "0.24.1",
    "snabbdom": "^0.7.0"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "coveralls": "^3.0.0",
    "mocha": "^4.0.0",
    "nyc": "^11.2.1",
    "prettier": "^1.7.4",
    "sinon": "^4.0.1"
  }
}
