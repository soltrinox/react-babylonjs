{
  "private": false,
  "license": "MIT",
  "name": "react-babylonjs-3d",
  "version": "0.2.0",
  "description": "BabylonJS 3D in a declarative way",
  "main": "./lib/react-babylon-3d.min.js",
  "keywords": [
    "babylonjs",
    "react",
    "jsx"
  ],
  "contributors": [
    {
      "name": "Vinicius Carneiro",
      "email": "vinicius.ribeiro.carneiro@gmail.com"
    }
  ],
  "scripts": {
    "build": "webpack",
    "lint": "eslint .",
    "test-only": "mocha --opts ./mocha.opts",
    "test-only-legacy": "mocha --opts test/mocha.opts",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "nyc mocha --opts ./mocha.opts",
    "prettier": "prettier --config ./.prettierrc --write \"{src,test,tests,mocks,examples}/**/*.js\""
  },
  "nyc": {
    "watermarks": {
      "lines": [
        80,
        95
      ],
      "functions": [
        80,
        95
      ],
      "branches": [
        80,
        95
      ],
      "statements": [
        80,
        95
      ]
    },
    "exclude": [
      "**/*.spec.js",
      "examples",
      "test"
    ],
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viniciusrmcarneiro/react-babylonjs.git"
  },
  "engines": {
    "node": ">= 8.2.1",
    "npm": ">= 5.30.0"
  },
  "author": "Vinicius R. M. Carneiro",
  "dependencies": {},
  "peerDepedencies": {
    "babylonjs": "3.*",
    "react-reconciler": "0.6.*",
    "react": "16.1.*"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "chai": "4.1.2",
    "coveralls": "^3.0.0",
    "eslint": "^4.16.0",
    "eslint-plugin-flowtype": "^2.41.0",
    "eslint-plugin-react": "^7.5.1",
    "mocha": "^5.0.0",
    "nyc": "^11.4.1",
    "prettier": "^1.10.2",
    "ramda": "^0.25.0",
    "react-devtools": "^3.0.0",
    "react-reconciler": "^0.7.0",
    "sinon": "^4.2.0",
    "webpack": "^3.10.0"
  }
}
