{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": ["last 2 versions", "safari 7"]
        },
        "commonjs": {
          "plugins": [["transform-es2015-modules-commonjs", { "loose": true }]]
        },
        "modules": false,
        "loose": true
      }
    ]
  ],
  "plugins": [
    [
      "transform-react-jsx",
      {
        "pragma": "hhh"
      }
    ]
  ]
}
