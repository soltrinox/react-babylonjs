{
  "private": false,
  "license": "MIT",
  "name": "react-babylonjs-3d",
  "version": "0.0.1-alpha4",
  "description": "BabylonJS 3D in a declarative way: CycleJS, ReactJS, RxJS",
  "main": "index.js",
  "scripts": {
    "test-only": "mocha --opts test/mocha.opts",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "nyc mocha --opts test/mocha.opts",
    "start": "webpack-dev-server --color --progress",
    "dev": "webpack --watch"
  },
  "nyc": {
    "watermarks": {
      "lines": [
        80,
        95
      ],
      "functions": [
        80,
        95
      ],
      "branches": [
        80,
        95
      ],
      "statements": [
        80,
        95
      ]
    },
    "exclude": [
      "**/*.spec.js",
      "examples",
      "test"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viniciusrmcarneiro/react-babylonjs.git"
  },
  "engines": {
    "node": ">= 8.2.1",
    "npm": ">= 5.30.0"
  },
  "author": "Vinicius R. M. Carneiro",
  "dependencies": {
    "babylonjs": "3.1.0-alpha2",
    "ramda": "0.24.1",
    "rxjs": "5.4.3",
    "snabbdom": "^0.7.0",
    "union-type": "0.4.0"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "chai": "4.1.2",
    "coveralls": "^3.0.0",
    "html-webpack-plugin": "2.30.1",
    "mocha": "3.5.3",
    "nyc": "^11.2.1",
    "sinon": "4.0.0",
    "webpack": "3.6.0",
    "webpack-dev-server": "2.9.1"
  }
}
